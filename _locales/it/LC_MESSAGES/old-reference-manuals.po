#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:64
msgid "referes to the ``Access contents information`` permission,"
msgstr ""

# 405ce1c202454ead89e26273153e7348
#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:65
msgid "**view**"
msgstr ""

# 45029824dadd471f9374e5e1f7c13468
#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:66
msgid "refers to the ``View`` permission,"
msgstr ""

# a245773755634927b54c1fa1fd3467a0
#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:67
msgid "**modify**"
msgstr ""

# d5de27574f6648aaa1b81fa807fc3e66
#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:68
msgid "refers to the ``Modify portal content`` permission,"
msgstr ""

# 9fd6445b895643388433b86dc68d65c2
#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:69
msgid "**list**"
msgstr ""

# b0e56bde84f640fea303cc3349cc0494
#: ../old-reference-manuals/archgenxml/basic-features/workflows.rst:70
msgid "refers to the ``List folder contents`` permission."
msgstr ""

